              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
	 LTRIM(RTRIM(e.LastName)) as LastName
	,LTRIM(RTRIM(e.FirstName)) as FirstName
	,LTRIM(RTRIM(e.EmpNo)) as EmpNo
	,e.OriginalHireDate
	,e.LastHireDate
	,e.JobCodeDescription
	,'cc' + PARSENAME(LEFT(CompDtl.DebitGLAccountSegment, LEN(CompDtl.DebitGLAccountSegment) - CHARINDEX('.', REVERSE(CompDtl.DebitGLAccountSegment))), 4) + '.' +
	  PARSENAME(LEFT(CompDtl.DebitGLAccountSegment, LEN(CompDtl.DebitGLAccountSegment) - CHARINDEX('.', REVERSE(CompDtl.DebitGLAccountSegment))), 3) as [ccCostCenter]
	,LTRIM(RTRIM(e.EmploymentStatus)) as EmploymentStatus
	,e.HourlyAmount
	,LTRIM(RTRIM(e.PayGradeCode)) as PayGradeCode
	,e.StandardUnitsPerWeek as FTEHrsPerWeek
	,e.AnnualSalary
	,e.FullName as EmployeeName
	,e.OriginalHireDate as HireDate
	,PARSENAME(LEFT(CompDtl.DebitGLAccountSegment, LEN(CompDtl.DebitGLAccountSegment) - CHARINDEX('.', REVERSE(CompDtl.DebitGLAccountSegment))), 4) as [CorpCode]
	,PARSENAME(LEFT(CompDtl.DebitGLAccountSegment, LEN(CompDtl.DebitGLAccountSegment) - CHARINDEX('.', REVERSE(CompDtl.DebitGLAccountSegment))), 3) as [CostCenter]
	,SUBSTRING(CompDtl.DebitGLAccountSegment, LEN(CompDtl.DebitGLAccountSegment) - CHARINDEX('.', REVERSE(CompDtl.DebitGLAccountSegment)) + 1 + 1, LEN(CompDtl.DebitGLAccountSegment)) as [JobCode]
	,pc.PayrollCode
	,CompDtl.CompensationDetailQuantity as [Hours]
	,CompDtl.PayRate as BaseRate
	,pt.PayrollTransactionPaymentDate as [PayDate]
	,CompDtl.CompensationDetailAmount as [Wages]
	--,CompDtl.DebitGLAccountSegment as [EOC_TEMP]
	,PARSENAME(LEFT(CompDtl.DebitGLAccountSegment, LEN(CompDtl.DebitGLAccountSegment) - CHARINDEX('.', REVERSE(CompDtl.DebitGLAccountSegment))), 2) as [EOC]
	,LEFT(CONVERT(VARCHAR, pt.PayrollTransactionPaymentDate, 107), 3) + ' ' + RIGHT(CONVERT(VARCHAR, pt.PayrollTransactionPaymentDate, 101), 2) as [Month]
	,CompDtl.CompensationDetailChargeDate
FROM [dbo].[EmployeePG2_Job_Curr] e 
	INNER JOIN dbo.tPayroll_Transactions pt
		ON pt.PersonIdNo = e.PersonIdNo
	INNER JOIN dbo.tCompensation_DETAILS CompDtl
		ON CompDtl.PayrollTransactionIdNo = pt.PayrollTransactionIdNo
	INNER JOIN dbo.tPAYROLL_CODES pc
		ON CompDtl.PayrollCodeIdNo = pc.PayrollCodeIdNo
WHERE 1=1
AND pt.PayrollTransactionPaymentDate &gt;= ?</property>
